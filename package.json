{
  "name": "webflow-colors-extension",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:cli": "concurrently -k -n build,serve \"vite build --watch\" \"webflow extension serve --dir dist\"",
    "build": "tsc -b && vite build",
    "bundle": "webflow extension bundle",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@types/node": "^22.5.2",
    "concurrently": "^9.2.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^2.1.1"
  }
}
